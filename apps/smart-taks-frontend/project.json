{
  "name": "smart-taks-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/smart-taks-frontend/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/smart-taks-frontend/browser",
        "index": "apps/smart-taks-frontend/src/index.html",
        "main": "apps/smart-taks-frontend/src/main.ts",
        "tsConfig": "apps/smart-taks-frontend/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/smart-taks-frontend/public"
          }
        ],
        "styles": ["apps/smart-taks-frontend/src/styles.css"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "smart-taks-frontend:build:production"
        },
        "development": {
          "buildTarget": "smart-taks-frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "smart-taks-frontend:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/smart-taks-frontend/jest.config.cts",
        "tsConfig": "apps/smart-taks-frontend/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "smart-taks-frontend:build",
        "port": 4200,
        "spa": true
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/smart-taks-frontend/server",
        "main": "apps/smart-taks-frontend/src/server.ts",
        "tsConfig": "apps/smart-taks-frontend/tsconfig.server.json"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "smart-taks-frontend:build:development",
          "serverTarget": "smart-taks-frontend:server:development"
        },
        "production": {
          "browserTarget": "smart-taks-frontend:build:production",
          "serverTarget": "smart-taks-frontend:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "development": {
          "browserTarget": "smart-taks-frontend:build:development",
          "serverTarget": "smart-taks-frontend:server:development"
        },
        "production": {
          "browserTarget": "smart-taks-frontend:build:production",
          "serverTarget": "smart-taks-frontend:server:production"
        }
      },
      "defaultConfiguration": "production"
    }
  }
}
